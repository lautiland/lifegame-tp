#ifndef PRINCIPAL_H_
#define PRINCIPAL_H_

enum SiguienteTurno {
	seguir,
	noSeguir
};

struct Informacion {
	float turno;
	float nacidasTotal;
	float muertasTotal;
	int vivasTotal;
	int nacidas;
	int muertas;
	SiguienteTurno sigTurno;

}datos;

#endif /* PRINCIPAL_H_ */
