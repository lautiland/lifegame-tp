#ifndef DATOSTURNO_H_
#define DATOSTURNO_H_

enum SiguienteTurno {
	seguir,
	noSeguir
};

struct Informacion {
	float turno;
	float nacidasTotal;
	float muertasTotal;
	int vivasTotal;
	int nacidas;
	int muertas;
	SiguienteTurno sigTurno;

}datos;

#endif /* DATOSTURNO_H_ */
